{
  "name": "expert-massage",
  "version": "2.6.1",
  "description": "Professional massage clinic Expert",
  "main": "app.js",
  "author": "Iurii Demidov",
  "repository": {
    "type": "git",
    "url": "https://github.com/YuryDemidov/Expert"
  },
  "private": true,
  "engines": {
    "node": "14.8.0"
  },
  "scripts": {
    "server": "npx nf start",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot --config=./webpack/dev.config.js",
    "watch": "npm run build:dev -- --watch",
    "watch-poll": "npm run build:dev -- --watch-poll",
    "build:dev": "cross-env NODE_ENV=development webpack --debug --config=./webpack/dev.config.js",
    "build:prod": "npm run sign-release && cross-env NODE_ENV=production webpack -p --bail --config=./webpack/prod.config.js",
    "sign-release": "git rev-parse HEAD | cut -c 1-8 > release.txt",
    "test:db:create": "WP_ENV=test wp db create",
    "test:db:snapshot": "wp db export tests/_data/dump.sql",
    "test:setup": "wp wordless theme setup_test_suite && composer install && npm run test:db:create && php ./vendor/bin/codecept build",
    "test:server": "npx nf start --procfile Procfile.testing --env .env.testing",
    "test": "vendor/bin/codecept run acceptance",
    "setup": "npm install && npm run test:setup"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.10",
    "autoprefixer": "^10.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.4.0",
    "cross-env": "^7.0.3",
    "css-loader": "^4.3.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.17.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "file-loader": "^6.2.0",
    "foreman": "^3.0.1",
    "html-webpack-plugin": "^4.5.1",
    "image-webpack-loader": "^7.0.1",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^0.11.3",
    "mocha": "^8.2.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.1.0",
    "postcss-scss": "^3.0.4",
    "pug": "^3.0.0",
    "pug-lint": "^2.6.0",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.1.0",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "sinon": "^9.2.3",
    "smoothscroll-polyfill": "^0.4.4",
    "style-loader": "^1.3.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "terser-webpack-plugin": "^2.3.8",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.0",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "core-js-bundle": "^3.8.2",
    "imask": "^6.0.5",
